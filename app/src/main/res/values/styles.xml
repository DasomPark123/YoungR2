<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="SplashTheme" parent="Theme.SubTheme">
        <item name="android:windowBackground">@drawable/splash</item>
    </style>
    <style name="Theme.AppCompat.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="col_black_12_font">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">12sp</item>
    </style>
    
    <style name="col_black_16_bold">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="col_black_14_bold">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    
    <style name="col_b5b5b5b5_12_font">
        <item name="android:textColor">@color/col_b5b5b5</item>
